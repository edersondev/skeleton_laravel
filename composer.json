{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories":{
        "popper":{
            "type":"package",
            "package":{
                "name":"FezVrasta/popper",
                "version":"1.14.3",
                "source":{
                    "url":"https://github.com/FezVrasta/popper.js",
                    "type":"git",
                    "reference":"1f75fbe"
                }
            }
        },
        "adminlte":{
            "type":"package",
            "package":{
                "name":"almasaeed2010/AdminLTE",
                "version":"3.0.0-alpha",
                "source":{
                    "url":"https://github.com/almasaeed2010/AdminLTE",
                    "type":"git",
                    "reference":"65f370e"
                }
            }
        },
        "FortAwesome":{
            "type":"package",
            "package":{
                "name":"FortAwesome/Font-Awesome",
                "version":"5.0.13",
                "source":{
                    "url":"https://github.com/FortAwesome/Font-Awesome",
                    "type":"git",
                    "reference":"bca574e"
                }
            }
        }
    },
    "require": {
        "php": "^7.1.3",
        "FezVrasta/popper": "1.14.3",
        "almasaeed2010/AdminLTE": "3.0.0-alpha",
        "ckeditor/ckeditor": "^4.9",
        "components/chosen": "^1.8",
        "datatables/datatables": "^1.10",
        "eternicode/bootstrap-datepicker": "^1.8",
        "fideloper/proxy": "^4.0",
        "igorescobar/jquery-mask-plugin": "^1.14",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "moment/moment": "^2.22",
        "pnikolov/bootbox": "^4.4",
        "twbs/bootstrap": "4.1.1",
        "yajra/laravel-datatables-oracle": "^8.5",
        "FortAwesome/Font-Awesome": "5.0.13"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
