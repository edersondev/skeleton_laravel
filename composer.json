{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories":{
        "adminlte":{
            "type":"package",
            "package":{
                "name":"almasaeed2010/AdminLTE",
                "version":"3.0.0-alpha-2",
                "source":{
                    "url":"https://github.com/almasaeed2010/AdminLTE",
                    "type":"git",
                    "reference":"7bf4139"
                }
            }
        },
        "FortAwesome":{
            "type":"package",
            "package":{
                "name":"FortAwesome/Font-Awesome",
                "version": "master",
                "source":{
                    "url":"https://github.com/FortAwesome/Font-Awesome",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        "titatoggle":{
            "type":"package",
            "package":{
                "name":"kleinejan/titatoggle",
                "version":"master",
                "source":{
                    "url":"https://github.com/kleinejan/titatoggle/",
                    "type":"git",
                    "reference":"master"
                }
            }
        },
        "jquery-dateFormat":{
            "type": "package",
            "package":{
                "name": "phstc/jquery-dateFormat",
                "version": "master",
                "source": {
                    "url": "https://github.com/phstc/jquery-dateFormat",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    },
    "require": {
        "php": "^7.1.3",
        "FortAwesome/Font-Awesome": "master",
        "almasaeed2010/AdminLTE": "3.0.0-alpha-2",
        "ckeditor/ckeditor": "^4.9",
        "components/chosen": "^1.8",
        "datatables/datatables": "^1.10",
        "eternicode/bootstrap-datepicker": "^1.8",
        "fideloper/proxy": "^4.0",
        "igorescobar/jquery-mask-plugin": "^1.14",
        "intervention/image": "^2.4",
        "kleinejan/titatoggle": "master",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "phstc/jquery-dateFormat": "master",
        "spatie/laravel-permission": "^2.12",
        "twbs/bootstrap": "4.1.1",
        "yajra/laravel-datatables-oracle": "^8.8"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
